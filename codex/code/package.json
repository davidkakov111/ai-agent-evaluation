{
  "name": "ai-evaluation",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "setup": "npm run prisma:generate && npm run prisma:migrate:dev && npm run prisma:seed",
    "typecheck": "tsc --noEmit",
    "build": "next build",
    "start": "next start",
    "start:prod": "npm run prisma:migrate:deploy && npm run start",
    "lint": "eslint . --max-warnings=0",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:seed": "node prisma/seed.mjs",
    "prisma:studio": "prisma studio",
    "prisma:validate": "prisma validate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/client": "^6.16.2",
    "@tanstack/react-query": "^5.90.21",
    "@trpc/client": "^11.10.0",
    "@trpc/react-query": "^11.10.0",
    "@trpc/server": "^11.10.0",
    "@trpc/tanstack-react-query": "^11.10.0",
    "bcrypt": "^6.0.0",
    "next": "16.1.6",
    "next-auth": "^4.24.13",
    "prisma": "^6.16.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
